<%= form_for @group, html: {multipart: true } do |f| %>

  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.date_field :start_date %>
  </p>

  <p>
    <%= f.date_field :end_date %>
  </p>

  <p>
    <%= f.label :size%><br>
    <%= f.number_field :size%>
  </p>

  <p>
    <%= f.label :course%><br>
    <%= f.collection_select :course, Course.all, :id, :name, prompt: true %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
